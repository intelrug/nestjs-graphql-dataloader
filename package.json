{
  "name": "@intelrug/nestjs-graphql-dataloader",
  "description": "Simplifies adding graphql/dataloader to your NestJS project",
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
