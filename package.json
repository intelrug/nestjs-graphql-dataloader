    "typescript": "^3.9.6",
    "graphql": "14.6.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.0.0"
  },
  "dependencies": {
    "rxjs": "^6.5.4",
